# Fullerstack SPI configuration for benchmark and TCK scripts
# Source this file or use these values when running jmh.sh/tck.sh

export SPI_GROUP=io.fullerstack
export SPI_ARTIFACT=fullerstack-substrates

# Extract version from pom.xml automatically
export SPI_VERSION=$(grep -m1 '<version>' pom.xml | sed 's/.*<version>\(.*\)<\/version>.*/\1/')
